CREATE MEMORY TABLE TORRENT_COMMENTS(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,CONTENT VARCHAR(255) NOT NULL,MODIFIED TIMESTAMP,POST_DATE TIMESTAMP NOT NULL,POSTER_ID BIGINT NOT NULL,TORRENT_ID BIGINT NOT NULL)
CREATE MEMORY TABLE TORRENTS(ID BIGINT NOT NULL PRIMARY KEY,VERSION BIGINT NOT NULL,CATEGORY_ID BIGINT NOT NULL,CHECKED BOOLEAN NOT NULL,DESCRIPTION VARCHAR(255),DOWNLOADED INTEGER NOT NULL,INFO_HASH VARCHAR(255) NOT NULL,LEECHERS INTEGER NOT NULL,NAME VARCHAR(255) NOT NULL,SEEDERS INTEGER NOT NULL,SIZE BIGINT NOT NULL,UPLOADED TIMESTAMP NOT NULL,UPLOADER_ID BIGINT NOT NULL,CONSTRAINT FKCADA89635CA6AF5A FOREIGN KEY(CATEGORY_ID) REFERENCES CATEGORY(ID))
CREATE MEMORY TABLE USERS(ID BIGINT NOT NULL PRIMARY KEY,ACCOUNT_EXPIRED BOOLEAN NOT NULL,ACCOUNT_LOCKED BOOLEAN NOT NULL,DOWNLOADED BIGINT NOT NULL,EMAIL VARCHAR(255) NOT NULL,ENABLED BOOLEAN NOT NULL,LASTLOGIN TIMESTAMP,"password" VARCHAR(255) NOT NULL,PASSWORD_EXPIRED BOOLEAN NOT NULL,POINTS INTEGER NOT NULL,REGISTRED TIMESTAMP NOT NULL,TITLE VARCHAR(255) NOT NULL,UPLOADED BIGINT NOT NULL,USERNAME VARCHAR(255) NOT NULL,CONSTRAINT SYS_CT_61 UNIQUE(USERNAME))
CREATE MEMORY TABLE USERS_ROLES(ROLES_ID BIGINT NOT NULL,USERS_ID BIGINT NOT NULL,PRIMARY KEY(ROLES_ID,USERS_ID),CONSTRAINT FKF6CCD9C611F5F23A FOREIGN KEY(ROLES_ID) REFERENCES ROLES(ID),CONSTRAINT FKF6CCD9C61223AA5A FOREIGN KEY(USERS_ID) REFERENCES USERS(ID))
