CREATE MEMORY TABLE BUGS(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,VERSION BIGINT NOT NULL,DESCRIPTION VARCHAR(255) NOT NULL,REPORTER_ID BIGINT NOT NULL,STATUS INTEGER NOT NULL,TITLE VARCHAR(255) NOT NULL)
