CREATE MEMORY TABLE NEWS(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,VERSION BIGINT NOT NULL,HTML_CONTENT VARCHAR(255) NOT NULL,TITLE VARCHAR(255) NOT NULL,UPLOADED TIMESTAMP NOT NULL,UPLOADER_ID BIGINT NOT NULL)
